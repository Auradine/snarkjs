    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "ptau 1st contrib",
            "skipFiles": [],
            "program": "cli.js",
            "args": [
                "powersoftau",
                "contribute",
                "pot12_0000.ptau",
                "pot12_0001.ptau",
                "--entropy=\"aaaaaaaa\"",
                "--name=\"First contribution\"",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ptau new",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "cli.js",
            "args": [
                "powersoftau",
                "new",
                "bn128",
                "12",
                "pot12_0000.ptau",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "r1cs",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "cli.js",
            "args": [
                "r1cs",
                "export",
                "json",
                "test/mycircuit/mycircuit.r1cs",
                "test/mycircuit/mycircuit.r1cs.json",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha all tests",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}