    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "zkey contribute",
            "skipFiles": [],
            "program": "cli.js",
            "args": [
                "zkey",
                "contribute",
                "mycircuit_0000.zkey",
                "mycircuit_0001.zkey",
                "--name=\"1st Contributor Name\"",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "groth16 setup",
            "skipFiles": [],
            "program": "cli.js",
            "args": [
                "groth16",
                "setup",
                "test/mycircuit/mycircuit.r1cs",
                "pot12_final.ptau",
                "mycircuit_0000.zkey"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ptau prepare phase2",
            "skipFiles": [],
            "program": "cli.js",
            "args": [
                "powersoftau",
                "prepare",
                "phase2",
                "pot12_0001.ptau",
                "pot12_final.ptau",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ptau 1st contrib",
            "skipFiles": [],
            "program": "cli.js",
            "args": [
                "powersoftau",
                "contribute",
                "pot12_0000.ptau",
                "pot12_0001.ptau",
                "--entropy=\"11111111\"",
                "--name=\"First contribution\"",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ptau new",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "cli.js",
            "args": [
                "powersoftau",
                "new",
                "bls12381",
                "2",
                "pot12_0000.ptau",
                "-v"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "r1cs",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "cli.js",
            "args": [
                "r1cs",
                "export",
                "json",
                "test/mycircuit/mycircuit.r1cs",
                "test/mycircuit/mycircuit.r1cs.json",
                "-v"
            ]
        }
    ]
